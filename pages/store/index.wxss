.wrap-input{
  display: flex;
  padding: 16px 0;
  background: #fff;
}
.weui-input{
  flex: 1;
  padding-right: 16px;
}

.item-icon{
  width: 24px;
  height: 24px;
  padding: 16px;
}
.item-txt{
  flex: 1;
}
.item-count{
  text-align: right;
  width: 50px;
}
.todo-wrap{
  padding-top: 90px;
}
.todo-item{
  background:#fff;
  padding-left: 16px;
  border-bottom: 1px solid #f5f5f5;
  height: 60px;
  line-height: 60px;
  display: flex;
}
.todo-item:last-child{
  border-bottom: none;
}

.icon-input{
  width: 16px;
  height: 16px;
  position: relative;
  top: 3px;
  padding: 0 16px;
}
.icon-add{
  width: 50px;
  height: 50px;
  position: fixed;
  right: 30px;
  bottom: 60px;
}
.tool{
  display: flex;
  justify-content: space-between;
  height: 36px;
  line-height: 36px;
  padding: 0 20px;
  background: #f5f5f5;
}

.item-tool{
  padding: 0 10px;
}
.add{
  color: #d81e06;
  font-weight: 900;
}
.fixed{
  position: fixed;
  top: 0;
  left: 0;
  width: 100%;
}

.null{
  display: flex;
  flex-direction: column;
  line-height:60px;
  align-items: center;
  color: #8a8a8a;
}

